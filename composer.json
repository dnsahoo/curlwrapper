{
	"name": "f3ath/curlwrapper",
	"type": "library",
	"description": "OOP wrapper for curl functions. Use CURL as a dependency, not hard-coded. Supports php up to 5.5.",
	"keywords": ["curl", "oop", "wrapper", "testing", "dependency injection"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Karapetov",
            "email": "karapetov@gmail.com",
            "role": "Developer"
        }
    ],
	"require": {
		"php": ">=5.3.0",
		"ext-curl": "*"
	},
	"require-dev": {
		"f3ath/fock": "*"
	},
	"autoload": {
        "psr-4": {
            "F3\\CurlWrapper\\": "src/"
        }
	}
}

