{
    "name": "f3ath/curlwrapper",
    "type": "library",
    "description": "The simplest OOP wrapper for curl, curl_multi, curl_share functions. Supports php up to 5.5, PSR-4. Use CURL as a dependency, not hard-coded!",
    "keywords": ["curl", "curl_multi", "curl_share", "oop", "wrapper", "testing", "dependency", "injection", "phpunit", "testable", "mock"],
    "license": "MIT",
    "authors": [
    {
        "name": "Alexey Karapetov",
        "email": "karapetov@gmail.com",
        "role": "Developer"
    }
    ],
        "require": {
            "php": ">=5.3.0",
            "ext-curl": "*"
        },
        "autoload": {
            "psr-4": {
                "F3\\CurlWrapper\\": "src/"
            }
        }
}
